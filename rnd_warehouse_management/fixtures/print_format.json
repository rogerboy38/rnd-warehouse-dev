[
	{
		"doctype": "Print Format",
		"name": "GI/GT Slip with Signatures",
		"doc_type": "Stock Entry",
		"module": "Warehouse Management",
		"standard": "No",
		"custom_format": 1,
		"disabled": 0,
		"print_format_builder": 0,
		"print_format_type": "Jinja",
		"html": "<!DOCTYPE html>\n<html>\n<head>\n\t<meta charset='utf-8'>\n\t<title>GI/GT Slip - {{ doc.name }}</title>\n\t<style>\n\t\tbody { font-family: Arial, sans-serif; font-size: 12px; margin: 20px; }\n\t\t.header { text-align: center; margin-bottom: 30px; border-bottom: 2px solid #333; padding-bottom: 10px; }\n\t\t.company-name { font-size: 18px; font-weight: bold; margin-bottom: 5px; }\n\t\t.document-title { font-size: 16px; font-weight: bold; color: #333; }\n\t\t.info-section { display: flex; justify-content: space-between; margin: 20px 0; }\n\t\t.info-box { border: 1px solid #ddd; padding: 10px; width: 48%; }\n\t\t.info-title { font-weight: bold; background-color: #f5f5f5; padding: 5px; margin: -10px -10px 10px -10px; }\n\t\t.field-row { margin: 8px 0; }\n\t\t.field-label { font-weight: bold; display: inline-block; width: 120px; }\n\t\t.field-value { display: inline-block; }\n\t\t.items-table { width: 100%; border-collapse: collapse; margin: 20px 0; }\n\t\t.items-table th, .items-table td { border: 1px solid #ddd; padding: 8px; text-align: left; }\n\t\t.items-table th { background-color: #f5f5f5; font-weight: bold; }\n\t\t.signature-section { display: flex; justify-content: space-between; margin: 30px 0; }\n\t\t.signature-box { border: 1px solid #ddd; padding: 15px; width: 45%; text-align: center; }\n\t\t.signature-title { font-weight: bold; margin-bottom: 10px; }\n\t\t.signature-image { max-width: 150px; max-height: 75px; margin: 10px 0; }\n\t\t.signature-line { border-bottom: 1px solid #333; height: 60px; margin: 10px 0; }\n\t\t.signature-date { font-size: 10px; margin-top: 10px; }\n\t\t.zone-status { padding: 5px 10px; border-radius: 3px; color: white; font-weight: bold; }\n\t\t.zone-red { background-color: #dc3545; }\n\t\t.zone-green { background-color: #28a745; }\n\t\t.sap-movement { background-color: #007bff; color: white; padding: 3px 8px; border-radius: 3px; font-weight: bold; }\n\t\t.footer { margin-top: 40px; text-align: center; font-size: 10px; color: #666; }\n\t\t@media print { body { margin: 0; } .no-print { display: none; } }\n\t</style>\n</head>\n<body>\n\t<!-- Header Section -->\n\t<div class='header'>\n\t\t<div class='company-name'>{{ doc.company }}</div>\n\t\t<div class='document-title'>Goods Issue / Goods Transfer Slip</div>\n\t\t<div style='margin-top: 10px;'>\n\t\t\t{% if doc.custom_gi_gt_slip_number %}\n\t\t\t\t<strong>Slip Number:</strong> {{ doc.custom_gi_gt_slip_number }}\n\t\t\t{% endif %}\n\t\t</div>\n\t</div>\n\n\t<!-- Document Information -->\n\t<div class='info-section'>\n\t\t<div class='info-box'>\n\t\t\t<div class='info-title'>Document Information</div>\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Document ID:</span>\n\t\t\t\t<span class='field-value'>{{ doc.name }}</span>\n\t\t\t</div>\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Date:</span>\n\t\t\t\t<span class='field-value'>{{ doc.posting_date.strftime('%d-%m-%Y') if doc.posting_date else '' }}</span>\n\t\t\t</div>\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Time:</span>\n\t\t\t\t<span class='field-value'>{{ doc.posting_time if doc.posting_time else '' }}</span>\n\t\t\t</div>\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Purpose:</span>\n\t\t\t\t<span class='field-value'>{{ doc.purpose }}</span>\n\t\t\t</div>\n\t\t\t{% if doc.custom_sap_movement_type %}\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>SAP Movement:</span>\n\t\t\t\t<span class='field-value'>\n\t\t\t\t\t<span class='sap-movement'>{{ doc.custom_sap_movement_type }}</span>\n\t\t\t\t\t{% if doc.custom_sap_movement_type == '261' %}(FrontFlush){% endif %}\n\t\t\t\t\t{% if doc.custom_sap_movement_type == '311' %}(BackFlush){% endif %}\n\t\t\t\t</span>\n\t\t\t</div>\n\t\t\t{% endif %}\n\t\t</div>\n\n\t\t<div class='info-box'>\n\t\t\t<div class='info-title'>Work Order Information</div>\n\t\t\t{% if doc.custom_work_order_reference %}\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Work Order:</span>\n\t\t\t\t<span class='field-value'>{{ doc.custom_work_order_reference }}</span>\n\t\t\t</div>\n\t\t\t{% endif %}\n\t\t\t{% if doc.custom_zone_status %}\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Zone Status:</span>\n\t\t\t\t<span class='field-value'>\n\t\t\t\t\t<span class='zone-status {% if doc.custom_zone_status == \"Red Zone\" %}zone-red{% else %}zone-green{% endif %}'>\n\t\t\t\t\t\t{{ doc.custom_zone_status }}\n\t\t\t\t\t</span>\n\t\t\t\t</span>\n\t\t\t</div>\n\t\t\t{% endif %}\n\t\t\t{% if doc.custom_material_completion_percentage %}\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Completion:</span>\n\t\t\t\t<span class='field-value'>{{ '%.1f'|format(doc.custom_material_completion_percentage) }}%</span>\n\t\t\t</div>\n\t\t\t{% endif %}\n\t\t\t<div class='field-row'>\n\t\t\t\t<span class='field-label'>Status:</span>\n\t\t\t\t<span class='field-value'>{{ doc.workflow_state or 'Draft' }}</span>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\n\t<!-- Items Table -->\n\t<table class='items-table'>\n\t\t<thead>\n\t\t\t<tr>\n\t\t\t\t<th style='width: 15%;'>Item Code</th>\n\t\t\t\t<th style='width: 25%;'>Item Name</th>\n\t\t\t\t<th style='width: 10%;'>Qty</th>\n\t\t\t\t<th style='width: 8%;'>UOM</th>\n\t\t\t\t<th style='width: 20%;'>Source Warehouse</th>\n\t\t\t\t<th style='width: 20%;'>Target Warehouse</th>\n\t\t\t\t<th style='width: 2%;'>Remarks</th>\n\t\t\t</tr>\n\t\t</thead>\n\t\t<tbody>\n\t\t\t{% for item in doc.items %}\n\t\t\t<tr>\n\t\t\t\t<td>{{ item.item_code }}</td>\n\t\t\t\t<td>{{ item.item_name }}</td>\n\t\t\t\t<td style='text-align: right;'>{{ '%.3f'|format(item.qty) }}</td>\n\t\t\t\t<td>{{ item.uom }}</td>\n\t\t\t\t<td>{{ item.s_warehouse or '-' }}</td>\n\t\t\t\t<td>{{ item.t_warehouse or '-' }}</td>\n\t\t\t\t<td>{{ item.description or '' }}</td>\n\t\t\t</tr>\n\t\t\t{% endfor %}\n\t\t</tbody>\n\t</table>\n\n\t<!-- Signature Section -->\n\t<div class='signature-section'>\n\t\t<div class='signature-box'>\n\t\t\t<div class='signature-title'>Warehouse Supervisor</div>\n\t\t\t{% if doc.custom_warehouse_supervisor_signature %}\n\t\t\t\t<img src='{{ doc.custom_warehouse_supervisor_signature }}' class='signature-image' alt='Warehouse Supervisor Signature'>\n\t\t\t\t{% if doc.custom_warehouse_supervisor_sign_date %}\n\t\t\t\t\t<div class='signature-date'>Signed on: {{ doc.custom_warehouse_supervisor_sign_date.strftime('%d-%m-%Y %H:%M') if doc.custom_warehouse_supervisor_sign_date else '' }}</div>\n\t\t\t\t{% endif %}\n\t\t\t{% else %}\n\t\t\t\t<div class='signature-line'></div>\n\t\t\t\t<div class='signature-date'>Signature & Date</div>\n\t\t\t{% endif %}\n\t\t</div>\n\n\t\t<div class='signature-box'>\n\t\t\t<div class='signature-title'>Kitting Supervisor</div>\n\t\t\t{% if doc.custom_kitting_supervisor_signature %}\n\t\t\t\t<img src='{{ doc.custom_kitting_supervisor_signature }}' class='signature-image' alt='Kitting Supervisor Signature'>\n\t\t\t\t{% if doc.custom_kitting_supervisor_sign_date %}\n\t\t\t\t\t<div class='signature-date'>Signed on: {{ doc.custom_kitting_supervisor_sign_date.strftime('%d-%m-%Y %H:%M') if doc.custom_kitting_supervisor_sign_date else '' }}</div>\n\t\t\t\t{% endif %}\n\t\t\t{% else %}\n\t\t\t\t<div class='signature-line'></div>\n\t\t\t\t<div class='signature-date'>Signature & Date</div>\n\t\t\t{% endif %}\n\t\t\t{% if doc.custom_sap_movement_type == '261' %}\n\t\t\t\t<div style='margin-top: 10px; font-size: 10px; color: #666;'>\n\t\t\t\t\t(Not required for SAP 261)\n\t\t\t\t</div>\n\t\t\t{% endif %}\n\t\t</div>\n\t</div>\n\n\t<!-- Additional Information -->\n\t{% if doc.remarks %}\n\t<div style='margin: 20px 0;'>\n\t\t<strong>Remarks:</strong> {{ doc.remarks }}\n\t</div>\n\t{% endif %}\n\n\t<!-- Footer -->\n\t<div class='footer'>\n\t\t<p>This document was generated automatically by RND Warehouse Management System</p>\n\t\t<p>Generated on: {{ frappe.utils.now_datetime().strftime('%d-%m-%Y %H:%M:%S') }}</p>\n\t\t{% if doc.custom_gi_gt_slip_generated_on %}\n\t\t<p>Original GI/GT Slip generated on: {{ doc.custom_gi_gt_slip_generated_on.strftime('%d-%m-%Y %H:%M:%S') if doc.custom_gi_gt_slip_generated_on else '' }}</p>\n\t\t{% endif %}\n\t</div>\n</body>\n</html>"
	}
]